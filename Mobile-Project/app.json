{
  "expo": {
    "name": "vitalhub",
    "slug": "vitalhub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashScreen.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,

      "bundleIdentifier": "com.misscoding.googleauthexporeactnativefilipe",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.814369502848-pti1e8rqipvsainns9orfbapstesgil4"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.misscoding.googleauthexporeactnativefilipe",

      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-font"]
  }
}
